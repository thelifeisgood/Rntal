<p id="notice"><%= notice %></p>

<h1>Biens</h1>

<table>
  <thead>
    <tr>
      <th>Typedebien</th>
      <th>Regimejuridique</th>
      <th>Nombredepieces</th>
      <th>Adresse</th>
      <th>Codepostal</th>
      <th>Ville</th>
      <th>Surfacehabitable</th>
      <th>Descriptiondubien</th>
      <th>Bailleur</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @biens.each do |bien| %>
      <tr>
        <td><%= bien.typeDeBien %></td>
        <td><%= bien.regimeJuridique %></td>
        <td><%= bien.nombreDePieces %></td>
        <td><%= bien.adresse %></td>
        <td><%= bien.codePostal %></td>
        <td><%= bien.ville %></td>
        <td><%= bien.surfaceHabitable %></td>
        <td><%= bien.descriptionDuBien %></td>
        <td><%= bien.bailleur %></td>
        <td><%= link_to 'Show', bailleur_bien_path(@bailleur, bien) %></td>
        <td><%= link_to 'Edit', edit_bailleur_bien_path(@bailleur, bien) %></td>
        <td><%= link_to 'Destroy', [@bailleur, bien], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

