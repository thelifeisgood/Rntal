<%= simple_form_for(@locataire) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :civilite %>
    <%= f.input :nom %>
    <%= f.input :prenom %>
    <%= f.input :nomDeux %>
    <%= f.input :prenomDeux %>
    <%= f.input :nomSociete %>
    <%= f.input :nRcs %>
    <%= f.input :adresseSiege %>
    <%= f.input :codePostalSiege %>
    <%= f.input :villeSiege %>
    <%= f.input :representant %>
    <%= f.association :bailleur %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
